export PATH=~/.local/bin:~/.local/share/bob/nvim-bin:~/.local/bin:$PATH

eval "$(atuin init bash --disable-up-arrow)"

alias ls="exa"
alias find="fd"
alias grep="rg"
alias cat="bat"
alias diff="delta"

alias ga="git add"
alias gap="ga -p"
alias gb="git branch"
alias gc="git commit"
alias gd="git diff"
alias gds="gd --staged"
alias gl="git log"
alias glo="gl --oneline"
alias go="git checkout"
alias gr="git restore"
alias grs="gr --staged"
alias gs="git status"

alias zynqmp_docs="google-chrome ~/dev/docs/zynqmp/*.pdf"

alias lldb="PYTHONPATH=/usr/lib/llvm-14/lib/python3.10/dist-packages lldb" # fix for lldb packages
